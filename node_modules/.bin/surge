#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules/surge/bin/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules/surge/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules/surge/bin/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules/surge/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/surge@0.24.6/node_modules:/Users/joseflorez/Sites/Proyecto-Marcos-Tejada/rodriguezcarvajal.com/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../surge/bin/surge" "$@"
else
  exec node  "$basedir/../surge/bin/surge" "$@"
fi
